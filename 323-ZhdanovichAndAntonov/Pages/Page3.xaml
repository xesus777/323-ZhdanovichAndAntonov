<Page x:Class="_323_ZhdanovichAndAntonov.Pages.Page3"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:_323_ZhdanovichAndAntonov.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Page3">

    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>

        
        <Menu Grid.Row="0" Background="LightGray">
            <MenuItem Header="Страницы">
                <MenuItem Header="Страница 1" Click="Page1_Click"/>
                <MenuItem Header="Страница 2" Click="Page2_Click"/>
                <MenuItem Header="Страница 3" Click="Page3_Click"/>
            </MenuItem>
            <MenuItem Header="Выход" Click="Exit_Click"/>
        </Menu>

        
        <Frame x:Name="MainFrame" Grid.Row="1" NavigationUIVisibility="Hidden"/>
    </Grid>
</Page>
